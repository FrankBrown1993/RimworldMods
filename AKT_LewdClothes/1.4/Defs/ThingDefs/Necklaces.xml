<Defs>
	
	<!-- Things saved for future upates
	
	
	
	hat/helms
	<hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
    <hatRenderedAboveBody>true</hatRenderedAboveBody>
    <hatRenderedBehindHead>false</hatRenderedBehindHead>
	
	ideology meme required for certain outfits
	<memePrerequisitesAny><li>Guilty</li><li>PainIsVirtue</li></memePrerequisitesAny>
	
	percept roles need edited to make a role wear a certain outfit found in percepts_role def
	
	
	
	<li MayRequire="Ludeon.RimWorld.Royalty">
	</li>
	
	<li MayRequire="Ludeon.RimWorld.Ideology">
	</li>
	
	-->>
	
	
  <!-- necklace A -->
  <ThingDef ParentName="AKTApparelMakeableBase">
		<defName>AKTNecklaceA</defName>
		<label>Round Pendant</label>
		<description>A small pendant adorned with a round disc, that rests neatly against your colonists' bosoms.</description>
		<graphicData>
			<texPath>Things/Pawn/Humanlike/Apparel/AKTNeckA/AKTNeckA</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costStuffCount>25</costStuffCount>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<recipeMaker>
			<researchPrerequisite>LewdJewelryResearch</researchPrerequisite>
			<unfinishedThingDef>UnfinishedSculpture</unfinishedThingDef>
			<useIngredientsForColor>true</useIngredientsForColor>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<effectWorking>Smith</effectWorking>
			<soundWorking>Recipe_Smith</soundWorking>
			<skillRequirements>
				<Crafting>5</Crafting>
			</skillRequirements>
		</recipeMaker>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>6000</WorkToMake>
			<Mass>0.1</Mass>
			<Beauty>0.2</Beauty>
			<StuffEffectMultiplierArmor>0</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<apparel>
			<bodyPartGroups>
				<li>Neck</li>
			</bodyPartGroups>
			<layers>
				<li>OnSkin</li>
			</layers>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/AKTNeckA/AKTNeckA</wornGraphicPath>
			<gender>Female</gender>
		</apparel>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
	</ThingDef>
  
  
