	<RecipeDef ParentName="RJWEX_MakeAdvBondageGear">
		<defName>MakeArmbinder</defName>
		<label>make armbinder</label>
		<description>Make an armbinder.</description>
		<jobString>Making armbinder.</jobString>
		<workAmount>5000</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Leathers</li>
						<li>Textiles</li>
					</categories>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>ResourcesRaw</li>
					</categories>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Hololock</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		<!--
			<categories>
				<li>Leathers</li>
			</categories>
		-->
		  <thingDefs>
				<li>Leather_Human</li>
				<li>Leather_Light</li>
				<li>Leather_Plain</li>
				<li>Synthread</li>
				<li>Hyperweave</li>
				<li>DevilstrandCloth</li>
				<li>Silver</li>
				<li>Gold</li>
				<li>Steel</li>
				<li>Plasteel</li>
				<li>Hololock</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Armbinder>1</Armbinder>
		</products>
		<skillRequirements>
			<Crafting>6</Crafting>
		</skillRequirements>
	</RecipeDef>

