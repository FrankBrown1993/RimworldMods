<Defs>
  <ThingDef Name="S16_MicroThong" ParentName="S16_Base">
    <defName>S16_MicroThong</defName>
    <label>micro thong</label>
    <description>Thin strip of cloth, accentuates rather then hides.</description>
    <graphicData>
      <texPath>Things/Apparel/Panties/MicroThong/S16Panties1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tradeability>All</tradeability>
	<costStuffCount>20</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
    </stuffCategories>
    <statBases>
      <WorkToMake>2000</WorkToMake>
      <Mass>0.1</Mass>
      <EquipDelay>1.0</EquipDelay>
	  <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.1</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.1</StuffEffectMultiplierInsulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <SexSatisfaction>0.2</SexSatisfaction>
    </equippedStatOffsets>
    <thingCategories>
      <li>S16_Underwear</li>
    </thingCategories>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Panties/MicroThong/S16Panties1</wornGraphicPath>
      <layers>
        <li>S16_Underwear</li>
      </layers>
      <tags>
        <li>S16_Panties</li>
		<li>S16_Tier1</li>
      </tags>
	  <gender>Female</gender>
    </apparel>
	<comps>
		<li Class="s16_extension.CompProperties_HediffApparel">
			<hediffDef>S16PantiesIHed</hediffDef>
			<partsToAffect>
					<li>Genitals</li>
			</partsToAffect>
		</li>
	<li><compClass>CompColorable</compClass></li>
    </comps>
	<recipeMaker>
	<researchPrerequisites>
      <li>S16_Apparel</li>
    </researchPrerequisites>
    </recipeMaker>
  </ThingDef>
  
