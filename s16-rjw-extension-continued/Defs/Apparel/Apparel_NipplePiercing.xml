    </graphicData>
    <techLevel>Industrial</techLevel>
    <tradeability>All</tradeability>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <WorkToMake>1500</WorkToMake>
      <Mass>0.05</Mass>
      <EquipDelay>5</EquipDelay>
	  <DeteriorationRate>1</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>S16_Piercing</li>
    </thingCategories>
    <equippedStatOffsets>
      <SexSatisfaction>0.1</SexSatisfaction>
      <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">0.02</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Nipple/WPins/S16WPins</wornGraphicPath>
      <layers>
        <li>S16_OnSkin</li>
      </layers>
      <tags>
        <li>S16_NipplePiercing</li>
		<li>S16_Tier0</li>
      </tags>
	  <gender>Female</gender>
    </apparel>
	<recipeMaker>
	  <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
      <skillRequirements>
        <Crafting>2</Crafting>
      </skillRequirements>
      <recipeUsers>
        <li>S16_CraftingSpot</li>
      </recipeUsers>
	 <researchPrerequisites>
	 <li>S16_Piercing</li>
	 <li>ComplexFurniture</li>
	 </researchPrerequisites>
    </recipeMaker>
     <costList>
      <WoodLog>5</WoodLog>
	  <Steel>5</Steel>
    </costList>
    <comps>
		<li Class="s16_extension.CompProperties_HediffApparel">
			<hediffDef>S16WPinsHed</hediffDef>
			<partsToAffect>
					<li>Chest</li>
			</partsToAffect>
		</li>
    </comps>
  </ThingDef>
 
