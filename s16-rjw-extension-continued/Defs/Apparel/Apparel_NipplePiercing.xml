<?xml version="1.0" encoding="utf-8" ?>
<Defs> 
  <ThingDef Name="S16_NippleRings" ParentName="S16_MetallicBase">
    <defName>S16_NippleRings</defName>
    <label>nipple rings</label>
    <description>Couple of simple nipple rings.</description>
    <graphicData>
      <texPath>Things/Apparel/Nipple/Simple/S16NippleRings</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <techLevel>Neolithic</techLevel>
    <costStuffCount>10</costStuffCount>
    <stuffCategories>
     <li>Metallic</li>
    </stuffCategories>
    <tradeability>All</tradeability>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>2500</WorkToMake>
      <Mass>0.05</Mass>
      <EquipDelay>5</EquipDelay>
	  <DeteriorationRate>1</DeteriorationRate>
    </statBases>
    <equippedStatOffsets>
      <SexSatisfaction>0.1</SexSatisfaction>
      <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">0.02</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <thingCategories>
      <li>S16_Piercing</li>
    </thingCategories>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Nipple/Simple/S16NippleRings</wornGraphicPath>
      <layers>
        <li>S16_OnSkin</li>
      </layers>
      <tags>
        <li>S16_NipplePiercing</li>
		<li>S16_Tier0</li>
      </tags>
	  <gender>Female</gender>
    </apparel>
    <recipeMaker>
      <recipeUsers>
		<li>S16_CraftingSpot</li>
      </recipeUsers>
	 <researchPrerequisites>
	 <li>S16_Piercing</li>
	 </researchPrerequisites>
    </recipeMaker>
	<comps>
		<li Class="s16_extension.CompProperties_HediffApparel">
			<hediffDef>S16NippleRingsHed</hediffDef>
			<partsToAffect>
					<li>Chest</li>
			</partsToAffect>
		</li>
    </comps>
  </ThingDef>
 
  <ThingDef Name="S16_WoodenPins" ParentName="S16_Base">
    <defName>S16_WoodenPins</defName>
    <label>wooden pins</label>
    <description>Useful for drying clothes. Some girls find them strange use.</description>
