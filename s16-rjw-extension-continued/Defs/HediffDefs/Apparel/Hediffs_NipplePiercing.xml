    <label>nipple rings</label>
    <description>Metal rings on nipples.</description>
    <isBad>false</isBad>
    <initialSeverity>0.01</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.20</severityPerDay>
      </li>
    </comps>
    <stages>
      <li>
	    <minSeverity>0.01</minSeverity>
        <label>Just put on</label>
		<painOffset>0.01</painOffset>
        <capMods>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.05</offset>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
		<statOffsets>
	     <SexFrequency>0.1</SexFrequency>
	     <!--SexAbility>.+</SexAbility-->
	     <Vulnerability>0.10</Vulnerability>
		</statOffsets>
      </li>
      <li>
        <minSeverity>0.20</minSeverity>
        <label>Aroused</label>
        <capMods>
		 <li>
            <capacity>Manipulation</capacity>
            <offset>-0.01</offset>
         </li>
         <li>
            <capacity>Moving</capacity>
            <offset>-0.01</offset>
         </li>
		</capMods>
		<statOffsets>
	     <SexFrequency>0.2</SexFrequency>
	     <!--SexAbility>.+</SexAbility-->
	     <Vulnerability>0.20</Vulnerability>
		</statOffsets>
      </li>
      <li>
        <minSeverity>0.60</minSeverity>
        <label>Comfortable</label>
        <capMods>
		 <li>
            <capacity>Manipulation</capacity>
            <offset>-0.01</offset>
         </li>
        </capMods>
		<statOffsets>
	     <SexFrequency>0.1</SexFrequency>
	     <!--SexAbility>.+</SexAbility-->
	     <Vulnerability>0.10</Vulnerability>
		</statOffsets>
      </li>
