		 <li>
            <capacity>Manipulation</capacity>
            <offset>-0.1</offset>
         </li>
         <li>
            <capacity>Moving</capacity>
            <offset>-0.1</offset>
         </li>
		</capMods>
		<statOffsets>
	     <Vulnerability>0.5</Vulnerability>
		</statOffsets>
      </li>
      <li>
        <minSeverity>0.40</minSeverity>
        <label>Comfortable</label>
		<painOffset>0.01</painOffset>
        <capMods>
		 <li>
            <capacity>Manipulation</capacity>
            <offset>-0.05</offset>
         </li>
		 <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
         </li>
        </capMods>
		<statOffsets>
	     <Vulnerability>0.5</Vulnerability>
		</statOffsets>
      </li>
    </stages>
  </HediffDef>
  
