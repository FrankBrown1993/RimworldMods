<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 <ThingDef ParentName="OrganicProductBase">
    <defName>S16LoveCookie</defName>
    <label>love cookie</label>
    <description>A cookie. Made with love.</description>
    <graphicData>
      <texPath>Things/Food/LoveCookie</texPath>
	  <graphicClass>Graphic_StackCount</graphicClass>
	  <drawSize>1.0</drawSize>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
	<stackLimit>10</stackLimit>
	<tickerType>Rare</tickerType>
	<statBases>
      <MarketValue>4.0</MarketValue>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Beauty>0</Beauty>
      <Mass>0.44</Mass>
      <DeteriorationRate>10</DeteriorationRate>
	  <Nutrition>0.9</Nutrition>
    </statBases>
	<tradeability>None</tradeability>
	<alwaysHaulable>true</alwaysHaulable>
	<comps>
      <li Class="CompProperties_Forbiddable" />
    </comps>
    <ingestible>
	  <foodType>Meal</foodType>
      <preferability>MealFine</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
	 <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>S16AteLoveCookie</hediffDef>
          <severity>0.50</severity>
        </li>
     </outcomeDoers>
    </ingestible>
    <thingCategories>
      <li>FoodMeals</li>
    </thingCategories>
  </ThingDef>
  
  <ThingDef ParentName="OrganicProductBase">
    <defName>S16LoveNectar</defName>
    <label>love nectar</label>
    <description>It's merely Love Nectar. Sweet and pure.</description>
    <graphicData>
      <texPath>Things/Food/LoveNectar</texPath>
	  <graphicClass>Graphic_StackCount</graphicClass>
	  <drawSize>0.9</drawSize>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
	<stackLimit>10</stackLimit>
	<tickerType>Rare</tickerType>
	<statBases>
      <MarketValue>4.0</MarketValue>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.1</Flammability>
      <Beauty>0</Beauty>
      <Mass>0.44</Mass>
      <DeteriorationRate>10</DeteriorationRate>
	  <Nutrition>0.5</Nutrition>
    </statBases>
	<tradeability>None</tradeability>
	<alwaysHaulable>true</alwaysHaulable>
	<comps>
      <li Class="CompProperties_Forbiddable" />
    </comps>
    <ingestible>
	  <foodType>Fluid</foodType>
      <preferability>MealSimple</preferability>
      <ingestSound>Ingest_Drink</ingestSound>
	 <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>S16AteLoveNectar</hediffDef>
          <severity>0.50</severity>
        </li>
     </outcomeDoers>
    </ingestible>
    <thingCategories>
      <li>FoodMeals</li>
    </thingCategories>
  </ThingDef>
</Defs>
